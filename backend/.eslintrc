{
  "env": {
    "browser": false,
    "mocha": false,
    "es6": true,
    "node": true
  },
  "parser": "babel-eslint",
  "extends": [ "airbnb/base", "prettier"],
  "rules": {
    "comma-dangle": ["error", "never"],
    "semi": [1, "never"],
    "no-console": 0,
    "class-methods-use-this": 0,
    "no-param-reassign" : 1
   }
}
